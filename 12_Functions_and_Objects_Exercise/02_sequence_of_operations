function processOperations(startValue, ...operations) {
    let currentValue = startValue;
    
    for (let operation of operations) {
        if (operation === "stop") {
            break;
        } else if (operation === "double") {
            currentValue *= 2;
        } else if (operation === "+1") {
            currentValue += 1;
        } else if (operation === "-1") {
            currentValue -= 1;
        } else if (operation === "print") {
            console.log(currentValue);
        } else {
            console.log("invalid operation");
        }
    }
}

// Example usage:
processOperations(
  20,
  "+1",
  "double",
  "print"
);

// Output: 42
// Start from value = 20
// Increment  value = 21
// Double  value = 42
// Print the current value  42
// No more operations  stop
